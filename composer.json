{
    "name": "emergentmethods/asknews-php-sdk",
    "description": "AskNews API [![status](https://status.asknews.app/api/badge/2/status?style=for-the-badge)](https://status.asknews.app/status/prod)",
    "keywords": [
        "newsapi",
        "llm",
        "rag",
        "syntheticdata",
        "news",
        "asknews"
    ],
    "homepage": "https://asknews.app",
    "license": "MIT",
    "authors": [
        {
            "name": "Emergent Methods, LLC",
            "homepage": "https://emergentmethods.ai"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2 || ^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "AskNews\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "AskNews\\Test\\" : "test/" }
    }
}
